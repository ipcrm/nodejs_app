{
  "name": "ipcrm-nodejs_app",
  "version": "0.0.1",
  "author": "ipcrm",
  "summary": "Application Orchestration Module for deploying Node.js applications",
  "license": "Apache-2.0",
  "source": "https://github.com/ipcrm/ipcrm-nodejs_app",
  "project_page": "https://github.com/ipcrm/ipcrm-nodejs_app",
  "issues_url": "https://github.com/ipcrm/ipcrm-nodejs_app/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.15.0"
    },
    {
      "name": "puppet-nodejs",
      "version_requirement": ">= 2.3.0"
    },
    {
      "name": "puppetlabs-app_modeling",
      "version_requirement": ">= 0.2.0"
    },
    {
      "name": "puppetlabs-vcsrepo",
      "version_requirement": ">= 1.5.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "windows",
      "operatingsystemrelease": [
        "2012 R2"
      ]
    }
  ]
}
